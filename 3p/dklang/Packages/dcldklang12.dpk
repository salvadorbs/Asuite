package dcldklang12;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $25200000}
{$DESCRIPTION 'DKLang Localization Package'}
{$DESIGNONLY}
{$IMPLICITBUILD ON}

requires
  designide,
  dklang12;

contains
  DKLangReg in '..\DKLangReg.pas',
  DKL_Expt in '..\DKL_Expt.pas',
  DKL_ConstEditor in '..\DKL_ConstEditor.pas' {dDKL_ConstEditor},
  DKL_ResFile in '..\DKL_ResFile.pas';

end.
